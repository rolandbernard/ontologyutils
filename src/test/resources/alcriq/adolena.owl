    <!-- file:/home/aurona/0AlleWerk/Navorsing/Ontologies/NAP/NAP#AssistiveDevice -->

    <owl:Class rdf:about="&NAP;AssistiveDevice">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&NAP;Device"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&NAP;ameliorates"/>
                        <owl:someValuesFrom rdf:resource="&NAP;Disability"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&NAP;assistsWith"/>
                        <owl:someValuesFrom rdf:resource="&NAP;Ability"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&NAP;Device"/>
    </owl:Class>
    


