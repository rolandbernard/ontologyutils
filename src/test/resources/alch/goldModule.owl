    </DatatypeProperty>
    


    <!-- http://purl.org/linguistics/gold/phoneticRep -->

    <DatatypeProperty rdf:about="&gold;phoneticRep">
        <rdfs:label xml:lang="eng">phoneticRep</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/linguistics/gold</rdfs:isDefinedBy>
