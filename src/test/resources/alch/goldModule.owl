    <DatatypeProperty rdf:about="&gold;abbreviation">
        <rdfs:label xml:lang="eng">abbreviation</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="eng">The abbreviated form representing a scientific term, e.g., ACC, 2, CL.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://purl.org/linguistics/gold/hasExample -->

    <DatatypeProperty rdf:about="&gold;hasExample">
        <rdfs:label xml:lang="eng">hasExample</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/linguistics/gold</rdfs:isDefinedBy>
    </DatatypeProperty>
    


    <!-- http://purl.org/linguistics/gold/orthographicRep -->

    <DatatypeProperty rdf:about="&gold;orthographicRep">
        <rdfs:label xml:lang="eng">orthographicRep</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:subPropertyOf rdf:resource="&gold;stringRep"/>
    </DatatypeProperty>
    


    <!-- http://purl.org/linguistics/gold/phonemicRep -->

    <DatatypeProperty rdf:about="&gold;phonemicRep">
        <rdfs:label xml:lang="eng">phonemicRep</rdfs:label>
